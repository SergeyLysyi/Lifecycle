<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="sergeylysyi.lifecycle">

    <application
        android:name="sergeylysyi.lifecycle.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="sergeylysyi.lifecycle.MainActivity"
            android:label="@string/first_activity_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="sergeylysyi.lifecycle.MainLogActivity"
            android:label="@string/second_activity_name" />
        <activity
            android:name="sergeylysyi.lifecycle.ViewGroupLogActivity"
            android:label="@string/third_activity_name" />
        <activity
            android:name="sergeylysyi.lifecycle.ButtonLogActivity"
            android:label="@string/fourth_activity_name" />
    </application>

</manifest>